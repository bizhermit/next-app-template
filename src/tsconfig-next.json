{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mts",
    "**/*.cts",
    ".next/types/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}