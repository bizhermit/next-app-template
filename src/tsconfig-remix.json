{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "types": [
      "@remix-run/node",
      "vite/client"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/.server/**/*.ts",
    "**/.server/**/*.tsx",
    "**/.client/**/*.ts",
    "**/.client/**/*.tsx",
    "../vite.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}